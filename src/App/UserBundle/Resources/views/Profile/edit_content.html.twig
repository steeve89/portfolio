{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('fos_user_profile_edit') }}" {{ form_enctype(form) }} method="POST" class="fos_user_profile_edit form-horizontal well">
    <fieldset>
        <legend class="text-center">{{ 'profile.edit.title' | trans }}</legend>
        {{ form_widget(form) }}
        <div class="form-action well text-center">
            <input class="btn btn-primary" type="submit" value="{{ 'profile.edit.submit'|trans }}" />
        </div>
    </fieldset>
</form>
